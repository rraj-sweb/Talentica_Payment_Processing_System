        <member name="M:PaymentProcessingWebAPI.Controllers.OrdersController.GetOrder(System.Guid)">
            <summary>
            Get detailed information about a specific order
            </summary>
            <param name="orderId">The unique identifier of the order</param>
            <returns>Order details including all associated transactions</returns>
            <response code="200">Order found and returned successfully</response>
            <response code="401">Unauthorized - JWT token required</response>
            <response code="404">Order not found</response>
        </member>
        <member name="M:PaymentProcessingWebAPI.Controllers.OrdersController.GetOrders(System.Int32,System.Int32)">
            <summary>
            Get a paginated list of orders
            </summary>
            <param name="page">Page number (default: 1, minimum: 1)</param>
            <param name="pageSize">Number of items per page (default: 10, maximum: 100)</param>
            <returns>List of orders with pagination</returns>
            <response code="200">Orders retrieved successfully</response>
            <response code="401">Unauthorized - JWT token required</response>
        </member>
        <member name="M:PaymentProcessingWebAPI.Controllers.OrdersController.GetOrderTransactions(System.Guid)">
